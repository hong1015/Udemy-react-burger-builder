     form = <Spinner />;\r\n    }\r\n    return (\r\n      <div className={classes.ContactData}>\r\n        {/* {reirect} */}\r\n        <h4>enter</h4>\r\n        {form}\r\n      </div>\r\n    );\r\n  }\r\n}\r\nconst mapStateToProps = (state) => {\r\n  // get the global state and reutrn it as a object that call ctr\r\n  return {\r\n    ingredientsState: state.burgerRed.ingredient,\r\n    totalPriceState: state.burgerRed.totalPrice,\r\n    loading: state.orderRed.loadingOrder,\r\n    // submitted: state.orderRed.orderSubmitted\r\n  };\r\n};\r\n\r\nconst mapDispatchToProps = (dispatch) => {\r\n  return {\r\n    postOrderHandler: (order) => dispatch(actionCreater.postOrderAction(order)),\r\n  };\r\n};\r\nexport default connect(\r\n  mapStateToProps,\r\n  mapDispatchToProps\r\n)(withErrorHandler(ContactData, axios));\r\n","C:\\Users\\God\\Documents\\react\\react-burger-builder\\src\\containers\\Auth\\Auth.js",[],"C:\\Users\\God\\Documents\\react\\react-burger-builder\\src\\store\\actions\\auth.js",[],"C:\\Users\\God\\Documents\\react\\react-burger-builder\\src\\store\\reducers\\auth.js",["164"],{"ruleId":"165","replacedBy":"166"},{"ruleId":"167","replacedBy":"168"},{"ruleId":"169","severity":1,"message":"170","line":1,"column":26,"nodeType":"171","messageId":"172","endLine":1,"endColumn":35},{"ruleId":"173","severity":1,"message":"174","line":31,"column":6,"nodeType":"175","endLine":31,"endColumn":8,"suggestions":"176"},{"ruleId":"169","severity":1,"message":"177","line":2,"column":10,"nodeType":"171","messageId":"172","endLine":2,"endColumn":18},{"ruleId":"173","severity":1,"message":"178","line":14,"column":6,"nodeType":"175","endLine":14,"endColumn":8,"suggestions":"179"},{"ruleId":"180","severity":1,"message":"181","line":17,"column":3,"nodeType":"182","messageId":"183","endLine":39,"endColumn":4},{"ruleId":"169","severity":1,"message":"184","line":7,"column":10,"nodeType":"171","messageId":"172","endLine":7,"endColumn":18},{"ruleId":"185","severity":1,"message":"18